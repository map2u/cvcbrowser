{
    "name": "symfony/framework-standard-edition",
    "license": "MIT",
    "type": "project",
    "description": "The \"Symfony Standard Edition\" distribution",
    "autoload": {
        "psr-0": {
            "": "src/"
        }
    },
    "require": {
        "php": ">=5.3.9",
        "symfony/symfony": "2.7.*",
        "doctrine/orm": "~2.2,>=2.2.3,<2.5",
        "doctrine/dbal": "<2.5",
        "doctrine/doctrine-bundle": "~1.4",
        "symfony/assetic-bundle": "~2.3",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "a2lix/i18n-doctrine-bundle": "dev-master",
        "a2lix/translation-form-bundle": "2.*@dev",
        "jms/serializer-bundle": "*",
        "paradigmate/image-bundle": "1.0.*",
        "friendsofsymfony/user-bundle": "1.3.x-dev as dev-master",
        "friendsofsymfony/jsrouting-bundle": "@stable",
        "sensio/distribution-bundle": "~4.0",
        "sensio/framework-extra-bundle": "~3.0,>=3.0.2",
        "knplabs/knp-menu": ">=2.0,<3.0",
        "knplabs/knp-menu-bundle": "dev-master",
        "knplabs/knp-paginator-bundle": "2.4.*",
        "knplabs/doctrine-behaviors": "dev-master",
        "sonata-project/core-bundle": "dev-master",
        "sonata-project/admin-bundle": "dev-master",
        "sonata-project/block-bundle": "dev-master",
        "sonata-project/media-bundle": "dev-master",
        "sonata-project/easy-extends-bundle": "dev-master",
        "sonata-project/formatter-bundle": "dev-master",
        "sonata-project/intl-bundle": "dev-master",
        "sonata-project/datagrid-bundle": "dev-master",
        "sonata-project/user-bundle": "dev-master",
        "sonata-project/doctrine-orm-admin-bundle": "dev-master",
        "sonata-project/news-bundle": "dev-master",
        "sonata-project/classification-bundle": "dev-master",
        "sonata-project/cache-bundle": "dev-master",
        "sonata-project/notification-bundle": "dev-master",
        "sonata-project/seo-bundle": "2.0.*@dev",
        "sonata-project/comment-bundle": "dev-master",
        "sonata-project/ecommerce": "dev-develop",
        "stof/doctrine-extensions-bundle": "~1.1@dev",
        "creof/doctrine2-spatial": "dev-master",
        "tecnickcom/tcpdf": "*",
        "excelwebzone/recaptcha-bundle": "2.6.x-dev",
        "lunetics/locale-bundle": "2.2.*",
        "hwi/oauth-bundle": "0.4.*@dev",
        "maxmind/geoip": "dev-master",
        "liip/theme-bundle": "dev-master",
        "suncat/mobile-detect-bundle": "0.10.*",
        "whiteoctober/breadcrumbs-bundle": "dev-master",
        "incenteev/composer-parameter-handler": "~2.0",
        "sensio/generator-bundle": "~2.3",
        "twig/twig": "1.*",
        "gedmo/doctrine-extensions": "2.3.*",
        "jms/di-extra-bundle": "~1.3",
        "ircmaxell/password-compat": "dev-master",
        "map2u/core-bundle": "dev-master",
        "map2u/leaflet-bundle": "dev-master",
        "map2u/web-bundle": "dev-master",
        "map2u/forum-bundle": "dev-master",
        "map2u/dashboard-bundle": "dev-master",
        "stfalcon/tinymce-bundle": "0.3.*@dev",
        "ornicar/akismet-bundle": "dev-master",
        "ibrows/newsletter-bundle": "2.1.*",
        "doctrine/doctrine-fixtures-bundle": "2.1.*@dev"

    },
    "repositories": [
        {
           "packagist": true  
        },
        {
            "type": "vcs",
            "url": "git@map2u.familyds.com:/volume1/homes/git/repositories/Map2uCoreBundle.git"
        },
        {
            "type": "vcs",
            "url": "git@map2u.familyds.com:/volume1/homes/git/repositories/Map2uLeafletBundle.git"
        },
        {
            "type": "vcs",
            "url": "git@map2u.familyds.com:/volume1/homes/git/repositories/Map2uMapfishPrintBundle.git"
        },
        {
            "type": "vcs",
            "url": "git@map2u.familyds.com:/volume1/homes/git/repositories/Map2uForumBundle.git"
        },
        {
            "type": "vcs",
            "url": "git@map2u.familyds.com:/volume1/homes/git/repositories/Map2uDashboardBundle.git"
        },
        {
            "type": "vcs",
            "url": "git@map2u.familyds.com:/volume1/homes/git/repositories/Map2uWebBundle.git"
        }],
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "config": {
        "bin-dir": "bin",
        "discard-changes": true
    },
    "minimum-stability": "stable",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "2.3.*@dev"
        }
    }
}
